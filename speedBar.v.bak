module speedBar(input wire animate, goAnimate, H_WIDTH, );

	wire [11:0] mx1, mx2, my1, my2

	rectangle #(.H_HIGHT(74), .H_WIDTH(16), .IX(515), .IY(351)) sP5mR (.i_animate(animate), .goAnimate(goAnimate), .o_x1(sP5mx1), .o_x2(sP5mx2), .o_y1(sP5my1), .o_y2(sP5my2));

	assign sPm = ((x>sP5mx1)&(y>sP5my1)&(x<sP5mx2)&(y<sP5my2)) ? 'h4 : 0;
	assign sPmf = ((x>sP5mx1+3)&(y>sP5my1+3)&(x<sP5mx2-3)&(y<sP5my2-3)) ? 'h1 : 0;

endmodule
